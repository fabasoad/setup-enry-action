{
  "name": "setup-enry-action",
  "version": "0.1.2",
  "description": "Setup Enry GitHub Action",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:fabasoad/setup-enry-action.git"
  },
  "scripts": {
    "build": "ncc build -m src/index.ts",
    "lint": "eslint --ext ts src",
    "postinstall": "pre-commit install --hook-type pre-commit && pre-commit install --hook-type pre-push",
    "test": "jest --config=jest.config.json --coverage"
  },
  "keywords": [
    "actions",
    "github-actions"
  ],
  "author": "Yevhen Fabizhevskyi",
  "license": "MIT",
  "homepage": "https://github.com/fabasoad/setup-enry-action#readme",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/tool-cache": "2.0.1",
    "winston": "3.8.2"
  },
  "devDependencies": {
    "@types/chai": "4.3.3",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.5",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "@vercel/ncc": "0.34.0",
    "chai": "4.3.6",
    "eslint": "8.25.0",
    "eslint-config-google": "0.14.0",
    "jest": "29.1.2",
    "jest-circus": "29.1.2",
    "mocha-param": "2.0.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}
